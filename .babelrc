{
  "presets": ["@babel/preset-env", "@babel/preset-react", "@babel/preset-typescript"],
  "plugins": [
    "emotion",
    [
      "@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ],
    [
      "babel-plugin-module-resolver",
      {
        "root": ["."],
        "alias": {
          "@": "./src",
          "@test": "./test"
        }
      }
    ]
  ]
}
